Host gitlab.com
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/flare

Host gitlab.com-isra
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/id_rsa

Host archisra
  RemoteForward 2224 127.0.0.1:2224
  RemoteForward 2225 127.0.0.1:2225
  SetEnv YANK_MODE=nc

Host dyn.isra17.xyz
  RemoteForward 2224 127.0.0.1:2224
  RemoteForward 2225 127.0.0.1:2225
  SetEnv YANK_MODE=nc
  Port 2222
