export CLICOLOR=1
export VISUAL="mvim -v -p -X"
export EDITOR="mvim -v -p -X"
alias vim='mvim -v'
alias vi='mvim -v'

### Added by the Heroku Toolbelt
export PATH="/usr/local/heroku/bin:$PATH"

### Set default docker
function start_docker {
  docker-machine start default
  eval "$(docker-machine env default)"
  echo Docker Ready!
}

[[ -z "$TMUX" ]] && exec tmux
